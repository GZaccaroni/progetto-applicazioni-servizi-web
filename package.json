{
  "name": "colture-in-cloud",
  "private": true,
  "scripts": {
    "clean": "rimraf \"packages/**/dist\" \"packages/**/*.tsbuildinfo\"",
    "prettier": "prettier \"*.{js,json,yml,md}\" \"packages/**/*\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "lint": "npm run format:check"
  },
  "workspaces": [
    "packages/*"
  ]
}
